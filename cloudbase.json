{
  "version": "2.0",
  "envId": "your-env-id",
  "$schema": "https://framework-1258016615.tcbapi.tencentcloudapi.com/schema/latest.json",
  "framework": {
    "name": "vite-react",
    "plugins": {
      "function": {
        "use": "@cloudbase/framework-plugin-function",
        "inputs": {}
      },
      "website": {
        "use": "@cloudbase/framework-plugin-website",
        "inputs": {
          "buildCommand": "npm run build",
          "outputPath": "dist"
        }
      }
    }
  },
  "functions": [
    {
      "name": "messages",
      "timeout": 5,
      "envVariables": {},
      "runtime": "Nodejs12.16",
      "handler": "index.main",
      "installDependency": true,
      "ignore": ["node_modules", "node_modules/**/*", ".git"]
    }
  ]
}

